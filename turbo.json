{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
		"build#ui": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"build#stylist": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
